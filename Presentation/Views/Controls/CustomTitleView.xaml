<?xml version="1.0" encoding="utf-8" ?>
<StackLayout x:Class="Maui.Ideas.App.Presentation.Views.Controls.CustomTitleView"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             HorizontalOptions="FillAndExpand"
             Orientation="Horizontal">

    <Label x:Name="titleLabel"
           Style="{DynamicResource Title_L}" />

    <ImageButton Clicked="ImageButton_Clicked"
                 Source="instagram_colored"
                 Style="{DynamicResource Button_M}" />
</StackLayout>
