<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="Maui.Ideas.App.Presentation.Views.Pages.StrategyPage"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Strategy"
             IconImageSource="settings_2_line"
             Style="{DynamicResource BasePageStyle}">
    <StackLayout Margin="48"
                 Orientation="Vertical"
                 Spacing="16"
                 VerticalOptions="Center">

        <Image HeightRequest="56"
               Source="settings_2_line"
               WidthRequest="56" />

        <Label FontFamily="LatoBold"
               FontSize="18"
               HorizontalTextAlignment="Center"
               Text="Strategy"
               VerticalTextAlignment="Center" />
    </StackLayout>
</ContentPage>